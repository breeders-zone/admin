{
  "name": "argon-dashboard-react",
  "version": "1.1.0",
  "description": "React version of Argon Dashboard by Creative Tim",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/argon-dashboard-react/issues"
  },
  "scripts": {
    "start": "set port=3001 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.13.1",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.19.2",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "connected-react-router": "^6.8.0",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.6",
    "draftjs-to-html": "^0.9.1",
    "formik": "^2.1.4",
    "html-to-draftjs": "^1.5.0",
    "moment": "^2.27.0",
    "node-sass": "4.14.1",
    "nouislider": "14.6.0",
    "qs": "^6.9.4",
    "react": "16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.1",
    "react-draft-wysiwyg": "^1.14.5",
    "react-dropzone": "^11.0.1",
    "react-google-maps": "9.4.5",
    "react-redux": "^7.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.1",
    "react-slick": "^0.26.1",
    "reactstrap": "8.5.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@types/googlemaps": "3.39.9",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.42",
    "eslint-plugin-flowtype": "5.2.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "memoize-one": "^5.1.1",
    "typescript": "3.9.6"
  }
}
